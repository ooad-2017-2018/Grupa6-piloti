        <Button Content="Dodavanje linija" HorizontalAlignment="Left" Margin="748,4,0,0" VerticalAlignment="Top" Height="45" Width="166"/>
        <Button Content="Informacije o aerodromu" HorizontalAlignment="Left" Margin="914,4,0,0" VerticalAlignment="Top" Height="45" Width="244"/>
        <Button Content="Izlaz" HorizontalAlignment="Left" Margin="1158,4,0,0" VerticalAlignment="Top" Height="45" Width="102" RenderTransformOrigin="-1.927,5.339"/>
